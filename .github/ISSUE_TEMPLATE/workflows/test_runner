name: PatMap Test Runners

on: 
  push:
    branches: ["main"]
    
jobs:
  patmap_test_runner:
    strategy:
      matrix:
        os: [windows-latest, ubuntu-latest, macos-latest]
        release: [2020b, 2021a, 2022a, ]
        
    name: Run MATLAB Script
    runs-on: ${{ matrix.os }}
    steps:
    -uses: actions/checkout@v3
    -name: Checkout
     uses: matlab-actions/setup-matlab@v1 ${{ release }}
     
